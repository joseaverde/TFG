[settings]
arch=x86_64
build_type=Debug
compiler=clang
compiler.cppstd=gnu17
compiler.libcxx=libstdc++11
compiler.version=16
os=Linux

[conf]
tools.cmake.cmake_layout:build_folder_vars=['const.tidy', 'settings.compiler', 'settings.arch']
tools.build:compiler_executables={"c":"/usr/bin/clang", "c++":"/usr/bin/clang++"}

[buildenv]
CC=/usr/bin/clang
CXX=/usr/bin/clang++
